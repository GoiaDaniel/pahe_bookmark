{
  "name": "Page Redder",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "js-loader.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["drawer.css"],
      "run_at": "document_end",
      "js": ["drawer.js"]
    }
  ]
}